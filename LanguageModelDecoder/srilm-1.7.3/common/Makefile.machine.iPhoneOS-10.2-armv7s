LOCAL_PLATFORM_NAME = iPhoneOS
XCODE_SDK_VERSION = 10.2
LOCAL_TARGET_ARCH = armv7s

include $(SRILM)/common/Makefile.core.iOS

ADDITIONAL_CFLAGS += -fembed-bitcode
ADDITIONAL_CXXFLAGS += -fembed-bitcode

